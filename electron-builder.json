{
  "appId": "tech.bscako.study.interpolate.teller",
  "productName": "BInterpolateTeller",
  "artifactName": "${productName}-${os}-${arch}-${version}.${ext}",
  "directories": {
    "output": "dist/release/b-interpolate-teller"
  },
  "files": [
      "**/*",
      "!**/*.ts",
      "!*.code-workspace",
      "!LICENSE.md",
      "!package-lock.json",
      "!src/",
      "!e2e/",
      "!.editorconfig",
      "!angular.json",
      "!package.json",
      "!tsconfig.json",
      "!tslint.json",
      "!yarn.lock",
      "!yarn-error.log",
      "!**/*.*.babel",
      "!./icons/",
      "!./ToDo"
  ],
  "win": {
    "icon": "dist/b-interpolate-teller",
    "target": [
      "portable"
    ]
  },
  "mac": {
    "icon": "dist/b-interpolate-teller",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist/b-interpolate-teller",
    "target": [
      "AppImage"
    ]
  }
}
