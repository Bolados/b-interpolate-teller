{
  "appId": "BInterpolateTeller",
  "category": "tech.bscako.interpolate.teller",
  "productName": "BInterpolateTeller",
  "directories": {
    "output": "release/b-interpolate-teller"
  },
  "files": [
      "**/*",
      "!**/*.ts",
      "!*.code-workspace",
      "!LICENSE.md",
      "!package-lock.json",
      "!src/",
      "!e2e/",
      "!.editorconfig",
      "!..gitignore",
      "!angular.json",
      "!package.json",
      "!tsconfig.json",
      "!tsconfig-electron.json",
      "!tslint.json",
      "!yarn.lock",
      "!yarn-error.log",
      "!electron/",
      "!node_modules/"
  ],
  "win": {
    "icon": "dist/b-interpolate-teller",
    "target": [
      "portable"
    ]
  },
  "mac": {
    "icon": "dist/b-interpolate-teller",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist/b-interpolate-teller",
    "target": [
      "AppImage"
    ]
  }
}
