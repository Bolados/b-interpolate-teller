  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "yarn run ng:serve",
    "build": "ng build",
    "build:prod": "yarn run build --prod",
    "build:prod:electron": "yarn run build:electron:tsconfig && yarn run build:prod --tsConfig=tsConfig-electron.json --base-href=./ --index=./electron/index.html",
    "build:electron:tsconfig": "tsc --p electron",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ng:serve": "ng serve",
    "electron:serve": "concurrently \"yarn run ng:serve\" \"yarn run build:electron:tsconfig && electron . --serve\" --kill-others",
    "electron:local": "yarn run build:prod:electron && electron .",
    "electron:pack:linux": "yarn run build:prod:electron && electron-builder build --linux",
    "electron:pack:windows": "yarn run build:prod:electron && electron-builder build --windows",
    "electron:pack:mac": "yarn run build:prod:electron && electron-builder build --mac"
  },
  "main": "./electron/dist/electron-main.js",
  "private": true,
